{
  "name": "health-calc-application",
  "version": "1.0.0",
  "type": "module",
  "description": "An application that calculates health metrics, using the body-measurements package",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,html,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css}\"",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . && npm run format:check",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:html": "htmlhint \"src/**/*.html\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Angelica Marker",
  "license": "ISC",
  "dependencies": {
    "body-measurements": "^1.0.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.8.1",
    "@typescript-eslint/parser": "^6.8.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "htmlhint": "^1.1.4",
    "prettier": "^3.3.3",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.3.3",
    "vite": "^4.5.0"
  }
}
