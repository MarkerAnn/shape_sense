{
  "name": "health-calc-application",
  "version": "1.0.0",
  "type": "module",
  "description": "An application that calculates health metrics, using the body-measurements package",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "format": "prettier --write 'src/**/*.{js,ts,html,css}'",
    "format:check": "prettier --check 'src/**/*.{js,ts,html,css}'",
    "lint:check": "eslint 'src/**/*.{js,ts}' && prettier --check 'src/**/*.{js,ts,html,css}'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Angelica Marker",
  "license": "ISC",
  "dependencies": {
    "body-measurements": "^1.0.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.8.1",
    "@typescript-eslint/parser": "^6.8.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  }
}
